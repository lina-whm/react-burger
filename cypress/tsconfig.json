{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"target": "es5",
		"lib": ["es5", "dom"],
		"types": ["cypress", "@testing-library/cypress"],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"baseUrl": "../node_modules",
		"paths": {
			"@/*": ["../src/*"]
		}
	},
	"include": ["**/*.ts", "../cypress.d.ts"]
}
